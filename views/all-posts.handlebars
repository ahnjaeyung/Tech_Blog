<section>
    {{#each posts as |post|}}
    <a href="/post/{{post.id}}">
    <div class="card">
        <div class="card-header">
            <h2> {{post.title}} </h2>
            <p> {{post.createdAt}} </p>
        </div>
        <div class="card-body">
            <p> {{post.body}} </p>
        </div>
    </div>
    </a>
    {{/each}}
</section>