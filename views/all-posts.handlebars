<section>
    {{#each posts as |post|}}
    <div>
        <a href="/post/{{post.id}}">
        <div>
            <h2> {{post.title}} </h2>
            <p> {{post.createdAt}} </p>
        </div>
        <div>
            <p> {{post.body}} </p>
        </div>
        </a>
    </div>
    {{/each}}
</section>